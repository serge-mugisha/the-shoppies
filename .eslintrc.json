{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": 0,
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-indent": 0,
        "indent": 0
    }
}
